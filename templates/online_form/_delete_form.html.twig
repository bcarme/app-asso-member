<form method="post" action="{{ path('online_form_delete', {'id': online_form.id}) }}" onsubmit="return confirm('Voulez-vous supprimer cete autorisation ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ online_form.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
