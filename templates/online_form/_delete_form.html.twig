<form method="post" action="{{ path('online_form_delete', {'id': online_form.id}) }}" onsubmit="return confirm('Voulez-vous supprimer ce document ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ online_form.id) }}">
    <button class="btn btn-danger"><i class="far fa-trash-alt mr-2"></i>Supprimer</button>
</form>
